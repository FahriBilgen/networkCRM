{
  "dashboard": {
    "id": null,
    "uid": "fortress-director",
    "title": "Fortress Director Monitoring",
    "tags": ["fortress-director", "production"],
    "timezone": "browser",
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Requests Total",
        "type": "gauge",
        "targets": [
          {
            "expr": "http_requests_total"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 6
        }
      },
      {
        "id": 2,
        "title": "HTTP Errors Total",
        "type": "gauge",
        "targets": [
          {
            "expr": "http_errors_total"
          }
        ],
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 6,
          "h": 6
        }
      },
      {
        "id": 3,
        "title": "Turn Executions Total",
        "type": "gauge",
        "targets": [
          {
            "expr": "turn_executions_total"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 6
        }
      },
      {
        "id": 4,
        "title": "Request Latency (ms)",
        "type": "graph",
        "targets": [
          {
            "expr": "http_request_duration_seconds * 1000"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 6,
          "w": 12,
          "h": 6
        }
      },
      {
        "id": 5,
        "title": "Turn Duration (ms)",
        "type": "graph",
        "targets": [
          {
            "expr": "turn_duration_seconds * 1000"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 6,
          "w": 12,
          "h": 6
        }
      },
      {
        "id": 6,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "cache_hit_rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "max": 1,
            "min": 0,
            "unit": "percentunit"
          }
        },
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 6,
          "h": 6
        }
      },
      {
        "id": 7,
        "title": "Process Uptime",
        "type": "gauge",
        "targets": [
          {
            "expr": "process_uptime_seconds / 3600"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "h"
          }
        },
        "gridPos": {
          "x": 6,
          "y": 12,
          "w": 6,
          "h": 6
        }
      },
      {
        "id": 8,
        "title": "Function Calls",
        "type": "table",
        "targets": [
          {
            "expr": "function_calls_total"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 12,
          "h": 6
        }
      }
    ]
  }
}
